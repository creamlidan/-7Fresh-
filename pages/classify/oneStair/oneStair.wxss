/* pages/classify/oneStair/oneStair.wxss */
.search{
	padding:24rpx;
	background:#fff;
	margin-bottom:24rpx;
}
.search navigator{
	background:#F3F3F3;
	height:54rpx;
	border-radius:100rpx;
	position:relative;
	padding-left:60rpx;
	color:#B2B2B2;
	font-size:22rpx;
	line-height:54rpx;
}
.search navigator image{
	position:absolute;
	left:24rpx;
	top:50%;
	width:24rpx;
	transform:translateY(-50%);
	-webkit-transform:translateY(-50%);
}
.classify_container{
	display:flex;
	flex-wrap:wrap;
}
.classify_container>navigator{
	width:50%;
	border-right:1px solid #eee;
	border-bottom:1px solid #eee;
	display:flex;
	align-items:center;
	justify-content:center;
	font-size:24rpx;
	color:#666;
	height:178rpx;
	background:#fff;
	box-sizing:border-box;
}
.classify_container>navigator:nth-child(2n){
	border-right:none;
}
.classify_container>navigator image{
	width:110rpx;
	margin-right:46rpx;
}
